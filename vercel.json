{
  "crons": [
    {
      "path": "/api/scheduler/init",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/scheduler/check",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/queue?action=process",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/check-completions",
      "schedule": "*/30 9-19 * * 1-6"
    },
    {
      "path": "/api/cron/send-reminders",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/cron/process-payments",
      "schedule": "*/15 * * * *"
    }
  ]
}