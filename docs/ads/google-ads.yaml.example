# Copy to google-ads.yaml and fill values. You can keep this file outside the repo and set
# the env var GOOGLE_ADS_CONFIGURATION_FILE to its absolute path.
# Docs: https://developers.google.com/google-ads/api/docs/client-libs/python/configuration

developer_token: "INSERT_DEVELOPER_TOKEN"
client_id: "INSERT_OAUTH_CLIENT_ID"
client_secret: "INSERT_OAUTH_CLIENT_SECRET"
refresh_token: "INSERT_REFRESH_TOKEN"
# Manager (MCC) account, numeric string without dashes
login_customer_id: "1234567890"
# Optional: direct customer_id if acting on a single account
# client_customer_id: "9876543210"
# Optional endpoint version; library defaults are fine
# use_proto_plus: True
