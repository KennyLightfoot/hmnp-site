{
  "name": "hmnp-site",
  "version": "0.1.0",
  "private": true,
  "comment": "Triggering a fresh build to run migrations",
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "setup-booking": "npx tsx scripts/setup-booking-system.ts",
    "check-booking": "npx tsx scripts/check-booking-health.ts",
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' prisma generate && cross-env NODE_OPTIONS='--max-old-space-size=8192' next build && next-sitemap",
    "build:safe": "cross-env SKIP_ENV_VALIDATION=true NEXT_PUBLIC_BASE_URL=https://houstonmobilenotarypros.com NODE_OPTIONS='--max-old-space-size=8192' prisma generate && cross-env SKIP_ENV_VALIDATION=true NEXT_PUBLIC_BASE_URL=https://houstonmobilenotarypros.com NODE_OPTIONS='--max-old-space-size=8192' next build && next-sitemap",
    "build:prod": "cross-env NODE_OPTIONS='--max-old-space-size=8192' pnpm run build",
    "build:fast": "cross-env NODE_OPTIONS='--max-old-space-size=8192' SKIP_TYPE_CHECK=true prisma generate && cross-env NODE_OPTIONS='--max-old-space-size=8192' next build && next-sitemap",
    "vercel-build": "pnpm db:migrate && pnpm build",
    "type-check": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit",
    "type-check:watch": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security-audit": "pnpm audit && pnpm audit --audit-level moderate",
    "start": "next start",
    "start:prod-dev": "NODE_ENV=production next dev",
    "studio": "sanity dev --port 3333",
    "studio:build": "sanity build",
    "studio:deploy": "sanity deploy",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:ci-smoke": "vitest run --maxConcurrency=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test": "vitest",
    "script:set-admin-password": "node --loader ts-node/esm scripts/set-admin-password.ts",
    "db:analyze": "node scripts/analyze-db-usage.cjs",
    "db:cleanup": "node scripts/cleanup-database.cjs",
    "db:cleanup:confirm": "node scripts/cleanup-database.cjs --confirm",
    "db:archive": "node scripts/archive-old-data.cjs",
    "db:archive:confirm": "node scripts/archive-old-data.cjs --confirm",
    "db:maintenance": "pnpm db:analyze && pnpm db:cleanup:confirm",
    "prisma:seed": "pnpm dotenv -e .env -- prisma db seed",
    "setup:booking": "node --loader ts-node/esm scripts/setup-booking-system.ts",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "db:migrate": "DATABASE_URL=$DIRECT_DATABASE_URL prisma migrate deploy",
    "db:seed": "prisma db seed",
    "setup-ghl-oauth": "node scripts/setup-ghl-oauth.js",
    "setup-ghl-calendars": "node scripts/setup-ghl-calendars.js",
    "verify-ghl-calendars": "node scripts/setup-ghl-calendars.js --verify-only",
    "booking:audit": "tsx scripts/audit-booking-config.ts",
    "booking:fix-hours": "tsx scripts/fix-business-hours.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
