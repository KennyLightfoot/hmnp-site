{
  "name": "hmnp-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' prisma generate && cross-env NODE_OPTIONS='--max-old-space-size=4096' next build && next-sitemap",
    "build:prod": "cross-env NODE_OPTIONS='--max-old-space-size=4096' pnpm run build",
    "build:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' SKIP_TYPE_CHECK=true prisma generate && cross-env NODE_OPTIONS='--max-old-space-size=4096' next build && next-sitemap",
    "type-check": "cross-env NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "script:set-admin-password": "node --loader ts-node/esm scripts/set-admin-password.ts",
    "db:analyze": "node scripts/analyze-db-usage.cjs",
    "db:cleanup": "node scripts/cleanup-database.cjs",
    "db:cleanup:confirm": "node scripts/cleanup-database.cjs --confirm",
    "db:archive": "node scripts/archive-old-data.cjs",
    "db:archive:confirm": "node scripts/archive-old-data.cjs --confirm",
    "db:maintenance": "pnpm db:analyze && pnpm db:cleanup:confirm",
    "prisma:seed": "pnpm dotenv -e .env -- prisma db seed",
    "setup:booking": "node --loader ts-node/esm scripts/setup-booking-system.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
