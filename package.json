  "scripts": {
    "dev": "next dev",
    "build": "pnpm dotenv -e .env -- prisma generate && pnpm dotenv -e .env -- bash -c 'DATABASE_URL=\"$DATABASE_URL_UNPOOLED\" pnpm prisma migrate deploy' && NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "script:set-admin-password": "node --loader ts-node/esm scripts/set-admin-password.ts"
  },
