{
  "name": "hmnp-site",
  "version": "0.1.0",
  "private": true,
  "comment": "Triggering a fresh build to run migrations",
  "type": "module",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "scripts": {
    "setup-booking": "npx tsx scripts/setup-booking-system.ts",
    "check-booking": "npx tsx scripts/check-booking-health.ts",
    "dev": "next dev",
    "build": "cross-env SILENCE_SERVER_INIT_LOGS=1 NODE_OPTIONS='--max-old-space-size=4096' prisma generate && cross-env SILENCE_SERVER_INIT_LOGS=1 NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint && next-sitemap",
    "build:safe": "cross-env SKIP_ENV_VALIDATION=true SILENCE_SERVER_INIT_LOGS=1 NEXT_PUBLIC_BASE_URL=https://houstonmobilenotarypros.com NODE_OPTIONS='--max-old-space-size=4096' prisma generate && cross-env SKIP_ENV_VALIDATION=true SILENCE_SERVER_INIT_LOGS=1 NEXT_PUBLIC_BASE_URL=https://houstonmobilenotarypros.com NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint && next-sitemap",
    "build:prod": "cross-env SILENCE_SERVER_INIT_LOGS=1 NODE_OPTIONS='--max-old-space-size=4096' pnpm run build",
    "build:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' SKIP_TYPE_CHECK=true prisma generate && cross-env NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint && next-sitemap",
    "vercel-build": "pnpm db:migrate && pnpm build",
    "type-check": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit",
    "type-check:watch": "cross-env NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security-audit": "pnpm audit && pnpm audit --audit-level moderate",
    "start": "next start",
    "start:prod-dev": "NODE_ENV=production next dev",
    "studio": "sanity dev --port 3333",
    "studio:build": "sanity build",
    "studio:deploy": "sanity deploy",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:integration": "TEST_TYPE=integration vitest run tests/api --coverage",
    "ci:verify": "pnpm type-check && pnpm lint && pnpm test:ci",
    "proof:smoke": "tsx scripts/proof-smoke.ts",
    "cleanup:prebooking": "tsx scripts/cleanup-prebooking-uploads.ts",
    "test:ci-smoke": "vitest run --maxConcurrency=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test": "vitest",
    "script:set-admin-password": "node --loader ts-node/esm scripts/set-admin-password.ts",
    "db:analyze": "node scripts/analyze-db-usage.cjs",
    "db:cleanup": "node scripts/cleanup-database.cjs",
    "db:cleanup:confirm": "node scripts/cleanup-database.cjs --confirm",
    "db:archive": "node scripts/archive-old-data.cjs",
    "db:archive:confirm": "node scripts/archive-old-data.cjs --confirm",
    "db:maintenance": "pnpm db:analyze && pnpm db:cleanup:confirm",
    "prisma:seed": "pnpm dotenv -e .env -- prisma db seed",
    "setup:booking": "node --loader ts-node/esm scripts/setup-booking-system.ts",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "db:migrate": "dotenv -e .env -- prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "setup-ghl-oauth": "node scripts/setup-ghl-oauth.js",
    "setup-ghl-calendars": "node scripts/setup-ghl-calendars.js",
    "verify-ghl-calendars": "node scripts/setup-ghl-calendars.js --verify-only",
    "booking:audit": "tsx scripts/audit-booking-config.ts",
    "booking:fix-hours": "tsx scripts/fix-business-hours.ts",
    "test:booking": "tsx scripts/test-booking-system.ts",
    "test:booking:quick": "pnpm test:unit --run tests/unit/booking-system.test.ts",
    "v0:check-deps": "tsx scripts/v0-integration.ts check-deps",
    "v0:validate": "tsx scripts/v0-integration.ts validate",
    "v0:integrate": "tsx scripts/v0-integration.ts integrate"
  },
